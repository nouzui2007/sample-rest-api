{
  "version": 2,
  "functions": {
    "index.js": {
      "runtime": "@vercel/node@18"
    }
  },
  "outputDirectory": ".",
  "includeFiles": [
    "data/**/*"
  ],
  "rewrites": [
    {
      "source": "/api",
      "destination": "/"
    },
    {
      "source": "/api/(.*)",
      "destination": "/"
    }
  ]
}
